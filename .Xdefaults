!Fonts

URxvt*font: xft:MonteCarlo:pixelsize=11:antialias=false, \
            [codeset=JISX0208]xft:Sucharaka Font:antialias=false,\
            [codeset=JISX0208]xft:Kochi Gothic:antialias=false
URxvt*boldFont: xft:MonteCarlo:pixelsize=11:antialias=false, \
            [codeset=JISX0208]xft:Sucharaka Font:antialias=true,\
            [codeset=JISX0208]xft:Kochi Gothic:antialias=false

XTerm*font: -windows-montecarlo-medium-r-normal--11-110-72-72-c-60-microsoft-cp1252
XTerm*allowBoldFonts: False

!General settings
URxvt*scrollBar: False
URxvt*scrollTtyOutput: False
URxvt*scrollTtyKeypress: True
URxvt*secondaryScroll: True
URxvt*saveLines: 1000
URxvt*geometry:84x24

!Xft settings
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.rgba: rgb


!Colors
*foreground: #eddec9
*background: #161616

! black
!URxvt.color0  : #343434
*color0  : #302e26
*color8  : #5c5748

! red
*color1  : #A7341b
*color9  : #7D2714

! green
*color2  : #878438
*color10 : #62712C

! yellow
*color3  : #ddb62b
*color11 : #B3921E

! blue
*color4  : #28617d
*color12 : #204D63

! magenta
*color5  : #802828
*color13 : #522226

! cyan
!*color6  : #60687f
!*color14 : #324466
*color6  : #302E26
*color14 : #5C5748

! white
URxvt.color7  : #eddec9
URxvt.color15 : #fafafa
