!
! General settings
!__________________

URxvt*scrollBar: False
URxvt*scrollTtyOutput: False
URxvt*scrollTtyKeypress: True
URxvt*secondaryScroll: True
URxvt*saveLines: 1000
URxvt*geometry:84x24



!
! Fonts
!_______

!URxvt*font: xft:Liberation Mono:pixelsize=11:antialias=false, \
!            [codeset=JISX0208]xft:Kochi Gothic:antialias=false
!URxvt*boldFont: xft:Liberation Mono:pixelsize=11:antialias=false, \
!            [codeset=JISX0208]xft:Kochi Gothic:antialias=false
URxvt*font: xft:MonteCarlo:pixelsize=11:antialias=false, \
            [codeset=JISX0208]xft:Kochi Gothic:antialias=false
URxvt*boldFont: xft:MonteCarlo:pixelsize=11:antialias=false, \
            [codeset=JISX0208]xft:Kochi Gothic:antialias=false

XTerm*font: -windows-montecarlo-medium-r-normal--11-110-72-72-c-60-microsoft-cp1252
XTerm*allowBoldFonts: False



!
! Xft settings
!______________

Xft.antialias: 1
Xft.autohint: 0
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb



!
! Colors
!________

*foreground: #ded0b4
*background: #201F1D

! black
!*color0  : #363126
*color0  : #332F27
*color8  : #5C5541

! red
*color1  : #9C3018
!*color1  : #A73D26
*color9  : #7D2714

! green
*color2  : #878438
*color10 : #62712C

! yellow
*color3  : #ddb62b
*color11 : #B3921E

! blue
*color4  : #2D83AD
*color12 : #204D63

! magenta
*color5  : #802828
*color13 : #522226

! cyan
*color6  : #3D444E
*color14 : #757A80

! white
URxvt.color7  : #eddec9
URxvt.color15 : #fafafa

! misc
URxvt.cursorColor : #eddec9
URxvt.highlightColor : #A7402A
